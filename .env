# Jive Flutter-Rust 环境配置

# API 服务端口
RUST_API_PORT=8012
FLUTTER_DEV_PORT=3020

# 数据库配置
DATABASE_URL=postgresql://localhost:5432/jive
POSTGRES_PORT=5432
POSTGRES_USER=jive
POSTGRES_PASSWORD=jive_password
POSTGRES_DB=jive

# Redis 配置（可选）
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379

# 日志级别
RUST_LOG=info
FLUTTER_LOG=info

# 开发模式
DEV_MODE=true

# API 密钥（生产环境请更改）
JWT_SECRET=your-secret-key-here
ENCRYPTION_KEY=your-256-bit-encryption-key-here