{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            code, \n            name, \n            name_zh,\n            symbol, \n            decimal_places, \n            is_active,\n            is_crypto,\n            flag,\n            COALESCE(country_code, '') as country_code,\n            COALESCE(is_popular, false) as is_popular,\n            COALESCE(display_order, 0) as display_order\n        FROM currencies\n        WHERE is_active = true\n        ORDER BY \n            is_crypto ASC,  -- Fiat first, then crypto\n            display_order ASC,\n            code ASC\n        ",
