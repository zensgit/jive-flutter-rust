  "query": "\n        SELECT l.id, l.family_id, l.name, l.currency, \n               l.is_default,\n               l.created_at, l.updated_at\n        FROM ledgers l\n        LEFT JOIN family_members fm ON l.family_id = fm.family_id\n        WHERE l.id = $1 AND (fm.user_id = $2 OR l.family_id IS NULL)\n        ",
