  "query": "\n        SELECT \n            code, \n            name, \n            name_zh,\n            COALESCE(symbol, '') AS symbol,\n            decimal_places, \n            is_active,\n            is_crypto,\n            flag\n        FROM currencies\n        WHERE is_active = true\n        ORDER BY \n            is_crypto ASC,  -- Fiat first, then crypto\n            code ASC\n        ",
