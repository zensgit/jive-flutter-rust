  "query": "\n        UPDATE ledgers\n        SET name = COALESCE($2, name),\n            currency = COALESCE($3, currency),\n            is_default = COALESCE($4, is_default),\n            updated_at = NOW()\n        WHERE id = $1\n        RETURNING id, family_id, name, currency, \n                  is_default,\n                  created_at, updated_at\n        ",
